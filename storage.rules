rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // Permite leer y escribir a cualquier usuario autenticado.
      // Ajusta esto seg√∫n tus necesidades de seguridad para el storage.
      allow read, write: if request.auth != null;
    }
  }
}
